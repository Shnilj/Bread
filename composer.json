{
  "name": "libaro/bread",
  "version": "1.0.0",
  "description": "Bread",
  "keywords": [
    "libaro",
    "laravel",
    "bread",
    "vue",
    "tailwindcss"
  ],
  "homepage": "https://github.com/libaro-io/Bread",
  "license": "MIT",
  "authors": [
    {
      "name": "Libaro",
      "email": "info@libaro.be",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^v7.5.0"
  },
  "autoload": {
    "psr-4": {
      "Libaro\\Bread\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Libaro\\Bread\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Libaro\\Bread\\BreadServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
